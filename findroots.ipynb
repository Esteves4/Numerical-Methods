    "def findroots(f, a, b, dx):\n",
    "    x1 = a\n",
    "    f1 = f(a)\n",
    "    x2 = a + dx\n",
    "    f2 = f(x2)\n",
    "\n",
    "    while f1*f2 >= 0:\n",
    "        if x1 >= b:\n",
    "            return None,None\n",
    "        x1 = x2\n",
    "        f1 = f2\n",
    "        x2 = x1 + dx\n",
    "        f2 = f(x2)\n",
    "    else:\n",
    "        return (x1,x2)"
   ]
  }
